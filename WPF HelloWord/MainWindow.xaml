<Window x:Name="winDown" x:Class="WPF_HelloWord.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WPF_HelloWord"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800"
        ResizeMode="NoResize">
    <Grid>
        <Button x:Name="btnResource" Content="Resource" HorizontalAlignment="Left" Margin="42,35,0,0" VerticalAlignment="Top" Width="90" Click="btnResource_Click"/>
        <Button x:Name="btnUICulture" Content="UICulture" HorizontalAlignment="Left" Margin="42,70,0,0" VerticalAlignment="Top" Width="90" Click="btnUICulture_Click"/>
        <Button x:Name="btnTextBlock" Content="TextBlock" HorizontalAlignment="Left" Margin="42,105,0,0" VerticalAlignment="Top" Width="90" Click="btnTextBlock_Click"/>

    </Grid>

</Window>
